{% block body %}

    <h3>Liste Commandes:</h3>


    <table border="2">
        <thead>
        <tr>

            <th>Prix Totale</th>
            <th>Adress Paiement</th>
            <th>Mail du Commande</th>


        </tr>
        </thead>
        <tbody>
        {% for commande in commandes %}
        <tr>

            <td>{{ commande.Prix }}</td>
            <td>{{ commande.adress }}</td>
            <td>{{ commande.Mail }}</td>
            {% endfor %}

        </tbody>

{% endblock %}